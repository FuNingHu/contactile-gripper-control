{
  "name": "contactile-gripper",
  "version": "0.0.0",
  "scripts": {
  "start": "npm run start --prefix contactile-gripper-frontend",
  "install-frontend": "npm install --prefix contactile-gripper-frontend",
  "lint": "npm run lint --prefix contactile-gripper-frontend",
  "build-frontend": "npm run build --prefix contactile-gripper-frontend",
  "copy-frontend": "ncp 'contactile-gripper-frontend/dist' 'dist'",
  "write-sbom": "npm run write-sbom-frontend",
  "write-sbom-frontend": "write-ur-packages ./contactile-gripper-frontend/package-lock.json dist",
  "install-backend": "npm install --prefix contactile-gripper-backend",
  "copy-backend": "ncp 'contactile-gripper-backend/dist' dist/contactile-gripper-backend",
  "build-backend": "npm run build --prefix contactile-gripper-backend",
  "prebuild": "npm run clean-dist && npm run clean-target && npm run validate-manifest",
  "build-arm": "export ARCH=linux/arm64 && npm run build",
  "postinstall": "npm run install-frontend && npm run install-backend",
  "build": "npm run build-backend && npm run build-frontend",
  "prepackage": "mkdir dist && npm run copy-backend && npm run copy-frontend && npm run copy-manifest && npm run copy-license && npm run write-sbom",
  "postbuild": "npm run package",
  "package": "package-urcap dist target",
  "copy-manifest": "ncp 'manifest.yaml' 'dist/manifest.yaml'",
  "copy-license": "ncp 'LICENSE' 'dist/LICENSE'",
  "clean-dist": "rimraf dist",
  "clean-target": "rimraf target",
  "install-urcap": "install-urcap 'target/contactile-gripper*.urcapx' '../manifest-spec-19.6.12.json'",
  "delete-urcap": "delete-urcap contactile contactile-gripper",
  "validate-manifest": "validate-manifest 'manifest.yaml' '../manifest-spec-19.6.12.json'"
},
  "private": true,
  "devDependencies": {
  "@universal-robots/urcap-utils": "1.6.4",
  "ncp": "2.0.0",
  "rimraf": "5.0.7"
  }
}
